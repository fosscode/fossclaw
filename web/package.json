{
  "name": "fossclaw",
  "version": "2.12.0",
  "type": "module",
  "description": "Web UI for launching and interacting with Claude Code agents",
  "license": "MIT",
  "author": "FossCode",
  "bin": {
    "fossclaw": "./bin/cli.ts"
  },
  "files": [
    "bin/",
    "server/",
    "dist/"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "agent",
    "web-ui",
    "fossclaw"
  ],
  "scripts": {
    "dev": "NODE_ENV=development bun server/index.ts",
    "dev:vite": "vite",
    "build": "vite build",
    "start": "NODE_ENV=production bun server/index.ts",
    "prepublishOnly": "bun run build",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "hono": "^4.7.0",
    "highlight.js": "^11.10.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/bun": "^1.2.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "zustand": "^5.0.0"
  }
}
